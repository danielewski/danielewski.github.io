# mod_rewrite starts here

RewriteEngine on

#does not apply to existing directiories, meaning that if the folder esists on the server than dont change anything and don't run the rule

RewriteCond %{REQUEST_FILENAME} !-d

#Check for file in directory with .html extention

RewriteCond %{REQUEST_FILENAME}\.html -f

# Acually show the page that has the .html extension

RewriteRule ^(.*)$ $1.html [NC, L]

